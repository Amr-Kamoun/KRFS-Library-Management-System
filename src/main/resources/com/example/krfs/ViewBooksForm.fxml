<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-image: url('file:BG.jpg'); -fx-background-size: 100% 100%;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.krfs.ViewBooksForm">
   <children>
      <TableView fx:id="Table" prefHeight="400.0" prefWidth="303.0">
        <columns>
          <TableColumn fx:id="Book" prefWidth="75.0" text="Book " />
            <TableColumn fx:id="Type" prefWidth="75.0" text="Type" />
            <TableColumn fx:id="Price" prefWidth="75.0" text="Price" />
            <TableColumn fx:id="author" prefWidth="75.0" text="author" />
        </columns>
      </TableView>
      <Button fx:id="logOutButton" layoutX="512.0" layoutY="369.0" mnemonicParsing="false" onMouseClicked="#logOutButtonClicked" prefHeight="26.0" prefWidth="88.0" style="-fx-background-color: #87CEFA;" text="Home" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="BorrowBook" layoutX="348.0" layoutY="153.0" mnemonicParsing="false" onMouseClicked="#Borrow_Book" prefHeight="46.0" prefWidth="208.0" style="-fx-background-color: #87CEFA;" text="Borrow Book" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
