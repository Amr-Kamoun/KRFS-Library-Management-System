<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-image: url('file:BG.jpg'); -fx-background-size: 100% 100%;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.krfs.ViewBooksAdmin">
   <children>
      <TableView fx:id="Table" prefHeight="400.0" prefWidth="302.0">
        <columns>
          <TableColumn fx:id="Book" prefWidth="75.0" text="Book " />
            <TableColumn fx:id="Price" prefWidth="75.0" text="Price" />
            <TableColumn fx:id="author" prefWidth="75.0" text="Author" />
            <TableColumn fx:id="Type" prefWidth="75.0" text="Type" />
        </columns>
      </TableView>
      <Button fx:id="logOutButton" layoutX="512.0" layoutY="369.0" mnemonicParsing="false" onMouseClicked="#logOutButtonClicked" prefHeight="26.0" prefWidth="88.0" style="-fx-background-color: #87CEFA;" text="Home" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="NewType" layoutX="378.0" layoutY="156.0" promptText="Type" />
      <TextField fx:id="NewPrice" layoutX="378.0" layoutY="89.0" promptText="Price" />
      <TextField fx:id="NewAuthor" layoutX="378.0" layoutY="124.0" promptText="Author" />
      <Button fx:id="AddButton" layoutX="404.0" layoutY="214.0" mnemonicParsing="false" onMouseClicked="#AddButtonClicked" text="Add" />
      <Button fx:id="RemoveButton" layoutX="485.0" layoutY="214.0" mnemonicParsing="false" onMouseClicked="#RemoveButtonClicked" text="Remove" />
      <TextField fx:id="NewName" layoutX="378.0" layoutY="55.0" promptText="Book" />
   </children>
</AnchorPane>
